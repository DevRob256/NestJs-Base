{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Nest Framework",
           "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "start:dev"
            ],
            "sourceMaps": true,
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "protocol": "inspector"
        }
    ]
  }